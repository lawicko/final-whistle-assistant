{
    "manifest_version": 3,
    "name": "Final Whistle Assistant",
    "version": "3.1.4",
    "description": "Displays additional information when browsing FinalWhistle website.",
    "icons": {
        "48": "icons/fw-logo-48.png",
        "96": "icons/fw-logo-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.finalwhistle.org/*"
            ],
            "js": [
                "content-scripts.bundle.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
    "background": __BACKGROUND_CONTENT__,
    "host_permissions": [
        "*://*.finalwhistle.org/*"
    ],
    "permissions": [
        "clipboardRead",
        "clipboardWrite",
        "contextMenus",
        "scripting",
        "storage",
        "tabs",
        "webNavigation"
    ],
    "options_ui": {
        "page": "options/options.html"
    }
}